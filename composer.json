{
    "name": "iamfredric/incognito",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fredric Lovefors",
            "email": "fredric@panang.se"
        }
    ],
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Incognito\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:iamfredric/modest.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:iamfredric/instantiator.git"
        }
    ],
    "require": {
        "iamfredric/modest": "0.1.1",
        "iamfredric/instantiator": "0.1.0",
        "duncan3dc/blade": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "mockery/mockery": "^0.9.9"
    }
}
